{% extends 'base.jinja' %}
{% block content %}
  <div
    class="flex flex-col justify-center text-center items-center justify-items-center gap-4 sm:p-20"
  >
    <h1>User List</h1>
    {% for user in users %}
      <div class="flex flex-col border p-2 rounded-xl gap-1 items-stretch">
        <span>{{ user["naam"] }}</span>
        <span>{{ user["email"] }}</span>
      </div>
    {% endfor %}
  </div>
{% endblock %}
